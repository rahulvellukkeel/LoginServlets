<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <servlet>
  	<servlet-name>log</servlet-name>
  	<servlet-class>com.main.Login</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>log</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>suc</servlet-name>
  	<servlet-class>com.main.SuccessServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>suc</servlet-name>
  	<url-pattern>/success</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>fail</servlet-name>
  	<servlet-class>com.main.FailureServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>fail</servlet-name>
  	<url-pattern>/fail</url-pattern>
  </servlet-mapping>
  
</web-app>